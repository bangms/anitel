package anitel.model;

public class roomDTO {
	private int room_num;
	private String id;
	private String name;
	private char pet_type;
	private String pet_etctype;
	private String d_fee;
	private char pet_big;
	private char util_pool;
	private char util_ground;
	private char util_parking;
	private char paid_bath;
	private char paid_beauty;
	private char paid_medi;
	private String img;
	public int getRoom_num() {
		return room_num;
	}
	public void setRoom_num(int room_num) {
		this.room_num = room_num;
	}
	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public char getPet_type() {
		return pet_type;
	}
	public void setPet_type(char pet_type) {
		this.pet_type = pet_type;
	}
	public String getPet_etctype() {
		return pet_etctype;
	}
	public void setPet_etctype(String pet_etctype) {
		this.pet_etctype = pet_etctype;
	}
	public String getD_fee() {
		return d_fee;
	}
	public void setD_fee(String d_fee) {
		this.d_fee = d_fee;
	}
	public char getPet_big() {
		return pet_big;
	}
	public void setPet_big(char pet_big) {
		this.pet_big = pet_big;
	}
	public char getUtil_pool() {
		return util_pool;
	}
	public void setUtil_pool(char util_pool) {
		this.util_pool = util_pool;
	}
	public char getUtil_ground() {
		return util_ground;
	}
	public void setUtil_ground(char util_ground) {
		this.util_ground = util_ground;
	}
	public char getUtil_parking() {
		return util_parking;
	}
	public void setUtil_parking(char util_parking) {
		this.util_parking = util_parking;
	}
	public char getPaid_bath() {
		return paid_bath;
	}
	public void setPaid_bath(char paid_bath) {
		this.paid_bath = paid_bath;
	}
	public char getPaid_beauty() {
		return paid_beauty;
	}
	public void setPaid_beauty(char paid_beauty) {
		this.paid_beauty = paid_beauty;
	}
	public char getPaid_medi() {
		return paid_medi;
	}
	public void setPaid_medi(char paid_medi) {
		this.paid_medi = paid_medi;
	}
	public String getImg() {
		return img;
	}
	public void setImg(String img) {
		this.img = img;
	}
	
	
}
